{
  "name": "MyContexts",
  "version": "0.24.3",
  "description": "The client for the Perspectives Distributed Runtime.",
  "private": true,
  "homepage": "https://mycontexts.com",
  "scripts": {
    "watch": "webpack --watch",
    "symlinks": "./createPerspectivesLinks.sh",
    "builddevelopment": "./build.sh -t development",
    "buildproduction": "./build.sh -t production",
    "buildRemoteTest": "./build.sh -t remotetest",
    "buildmodels_prod": "webpack --watch --config models.webpack.config.js --env target=production repo=https://perspectives.domains:5987/models_perspectives_domains",
    "buildTestStaticScreen": "webpack --watch --config testStaticScreen.webpack.config.js",
    "switchToProduction": "sudo ln -sfh /Users/joopringelberg/Code/inplace/production/ /Library/WebServer/Documents/mycontexts",
    "switchToDevelopment": "sudo ln -sfh /Users/joopringelberg/Code/inplace/development/ /Library/WebServer/Documents/mycontexts",
    "publishRemoteTest": "scp -r ./remotetest/* joop@inplace.works:/var/www/mycontexts.com/remotetest",
    "publishProduction": "scp -r ./production/* joop@inplace.works:/var/www/mycontexts.com",
    "publishDevelopment": "scp -r ./development/* joop@inplace.works:/var/www/mycontexts.com",
    "stats": "webpack --env target=development --json > stats.json"
  },
  "author": "Joop Ringelberg <joop.ringelberg@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joopringelberg/inplace.git"
  },
  "browserslist": [
    "defaults"
  ],
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@primer/octicons-react": "^19.12.0",
    "bootstrap": "^5.3.3",
    "highlight.js": "^11.10.0",
    "i18next": "^23.16.0",
    "i18next-http-backend": "^2.6.2",
    "idb-keyval": "^6.2.1",
    "perspectives-core": "git+https://github.com/joopringelberg/perspectives-core.git#v0.26.5",
    "perspectives-highlightjs": "github:joopringelberg/perspectives-highlightjs#v1.3.1",
    "perspectives-pageworker": "git+https://github.com/joopringelberg/perspectives-pageworker1.git#v0.16.5",
    "perspectives-proxy": "git+https://github.com/joopringelberg/perspectives-proxy.git#v1.21.4",
    "perspectives-react": "https://github.com/joopringelberg/perspectives-react.git#v0.26.3",
    "perspectives-sharedworker": "git+https://github.com/joopringelberg/perspectives-sharedworker.git#v0.16.5",
    "pouchdb-browser": "^9.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.5",
    "react-bootstrap-typeahead": "^6.3.2",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.31.1",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
