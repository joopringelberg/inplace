    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.12.0",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^5.5.0",
    "ignore-loader": "^0.1.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.26.3",
    "webpack-cli": "^5.0.1"
  }
}
